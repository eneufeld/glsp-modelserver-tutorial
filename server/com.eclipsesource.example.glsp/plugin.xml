<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.eclipsesource.example.glsp.GLSPApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.eclipsesource.example.glsp.application"
            name="glsp">
         <property
               name="appName"
               value="glsp">
         </property>
      </product>
   </extension>

</plugin>
